{
  "comments": false,
  "presets":[["@babel/preset-env",
    {
      "targets":{
        "node": true
      }
      ,
      "useBuiltIns": "usage" 
    }]],
    "plugins": [
      ["@babel/plugin-transform-runtime", {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }],
      ["transform-imports", {
        "lodash": {
          "transform": "lodash/${member}",
          "preventFullImport": true
        }
      }]
  ]
}
